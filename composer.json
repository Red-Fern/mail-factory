{
    "name": "red-fern/mail-factory",
    "description": "Send test mails generated with factory data using a simple artisan command.",
    "authors": [
        {
            "name": "Liam O'Connor",
            "email": "liam@red-fern.co.uk"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "3.8.*|4.*|5.*",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "RedFern\\MailFactory\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RedFern\\MailFactory\\MailFactoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
